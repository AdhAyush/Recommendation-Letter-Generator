{
    "phases": {
        "install": {
            "cmds": [
                "pip install -r requirements.txt",
                "npm install"
            ]
        },
        "build": {
            "cmds": [
                "python manage.py migrate",
                "python manage.py collectstatic --noinput"
            ]
        }
    },
    "start": {
        "cmds": "python manage.py runserver"
    }

}
